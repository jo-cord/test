function toCelsius(fahrenheit) {
  return (5/9) * (fahrenheit - 32);
}

let value1 = toCelsius(10);
let value2 = toCelsius;

console.log(typeof value1, typeof value2);


// antwoord: "number function"
// Uitleg:
// value1 is het resultaat van de functie-aanroep toCelsius(10), wat een getal (number) oplevert.
// value2 is een verwijzing naar de functie zelf, zonder deze aan te roepen, dus het is van het type function.